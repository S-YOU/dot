lines = DATA.split("\n")
records = []
dict = {}

for line in lines:
    fields = line.split(None, 2)
    if len(fields) == 3:
        dict[fields[1]] = fields[2]

termcap = "/etc/termcap"
f = open(termcap, "r")
lines = f.readlines()

def lis_compare(a, b):
    return cmp(a[2], b[2])
    
lis = []
print """
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>""" + searchEntry + """のエスケープシーケンス</title>
</head>
"""
print "<table>"
print "<h1>%s</h1>" % (searchEntry)
for i in xrange(len(lines)):
    line = lines[i]
    if line.startswith(searchEntry + "|"):
        for j in xrange(i + 1, len(lines)):
            line2 = lines[j]
            line2 = line2.rstrip("\\\n")
            if not line2.startswith("\t"):
                lis.sort(lis_compare)
                for x in lis:
                    print "<tr><td>%s</td><td>%s</td><td>%s</td></tr>" % (x[0], x[1], x[2])

                print "</table>"
                print """
                </body>
                </html>
                """
                sys.exit(0)
            a = line2.split(":")
            for cap in a:
                cap = cap.strip()
                if cap == "":
                    continue
                elif "=" in cap:
                    (name, val) = cap.split("=", 1)
                else:
                    name = cap
                    val = "オン"
                desc = dict.get(name, name)
                lis.append((desc, name, val))
                #print "<tr><td>%s</td><td>%s</td><td>%s</td></tr>" % (desc, name, val)
