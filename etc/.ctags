--langmap=scheme:
--langdef=scm
--langmap=scm:.ss.scm
--langmap=php:+.inc.html
--PHP-kinds=cdfi
--regex-scm=/^[[:space:]]*(\(|[[])define[^[:space:]]*[[:space:]]+(\(|[[])?([^][[:space:]()]+)/\3/d,definition/i
--exclude=.svn
--exclude=.git
--exclude=.bundle
--exclude=bundle
--exclude=node_modules

--regex-ruby=/^[ \t]*([A-Z_][A-Z0-9_]*)[ \t]*=/\1/C,constant/

--langdef=objc
--langmap=objc:.m
--regex-objc=/^\s*[-+]\s*\([[:alpha:]]+\s*\*?\)\s*([[:alnum:]]+):\s*\(/\1/m,method/
--regex-objc=/^\s*[-+]\s*\([[:alpha:]]+\s*\*?\)\s*([[:alnum:]]+)\s*\{/\1/m,method/
--regex-objc=/^\s*[-+]\s*\([[:alpha:]]+\s*\*?\)\s*([[:alnum:]]+)\s*\;/\1/m,method/
--regex-objc=/^\s*\@property\s+.*\s+\*?(.*);$/\1/p,property/
--regex-objc=/^\s*\@implementation\s+(.*)$/\1/c,class/
--regex-objc=/^\s*\@interface\s+(.*)\s+:.*\{/\1/i,interface/
